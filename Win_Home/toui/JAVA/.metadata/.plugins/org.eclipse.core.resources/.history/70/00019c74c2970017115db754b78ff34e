package jp.co.doraxdora.form;

import java.util.ArrayList;

public class Calculation {
	
	public int Search(ArrayList<Integer> list, int Data)
	{
		if (list.size() == 0 )
			return 0;
		int index = list.size();
		for (int i=index; i>=0; i--)
		{
			if(Data < list.get(i-1) && Data > list.get(i))
			{;
				return i;
			}
		}
		return 0;
	}

}
