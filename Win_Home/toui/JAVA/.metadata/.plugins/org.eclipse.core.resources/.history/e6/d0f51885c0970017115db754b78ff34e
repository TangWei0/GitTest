package jp.co.doraxdora.form;

import java.util.ArrayList;

public class Calculation {
	
	public int Search(ArrayList<Integer> list, int Data)
	{
		int index = 0;
		if (list.size() == 0 )
			return 0;
		for (int i=0; i<list.size(); i++)
		{
			if(Data > list.get(i))
			{
				index = i;
			}
		}
		return index;
	}

}
